<body>
    <header>
        <a href="{{url_for('index')}}"><img alt=" " src="/static/img/img_index/logo.png"></a><br>
    </header>
    {%if session%}
    <div class="usuarioSesion">
        <a style="text-decoration: none; color: white;" href="{{url_for('MiPerfil', id=session['id'])}}"><img src="{{session['imgUrl']}}"><br></a>
        <label>{{session['name']}}</label>
    </div>
    {%endif%}
    <nav>
        {%if session%}
        <ul>
            <li><span><a href="{{url_for('community')}}">COMUNIDAD</a></span></li>
            <li><span><a href="{{url_for('index')}}">PRINCIPAL</a></span></li>
            <li><span><a href="{{url_for('tiket')}}">SOPORTE</a></span></li>
            <li><span><a href="{{url_for('logout')}}">CERRAR SESION</a></span></li>
            {% if session['role'] == 'Ejecutivo' or session['role'] == 'Staff' %}
                <li><span><a href="{{url_for('minecraft.GestionarSectoresPerdidos')}}">SECTORES PERDIDOS</a></span></li>
                <li><span><a href="{{url_for('minecraft.GestionarMisiones')}}">MISIONES</a></span></li>
            {% endif %}
        </ul>
        {%else%}
        <ul>
            <li><span><a href="{{url_for('community')}}">COMUNIDAD</a></span></li>
            <li><span><a href="{{url_for('index')}}">PRINCIPAL</a></span></li>
            <li><span><a href="{{url_for('login')}}">INICIAR SESION</a></span></li>
            <li><span><a href="{{url_for('register')}}">REGISTRAR</a></span></li>
        </ul>
        {%endif%}
    </nav>
    <main>
        {%block content%} {%endblock%}
    </main>
    <footer>
    
    </footer>
</body>