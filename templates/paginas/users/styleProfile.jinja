{%extends 'base.jinja'%}

{%block title%} Editar estilo perfil {%endblock%}

{%block content%}
<br><br><br>
<br><br><br>
<div style="text-align: center;">
    <a href="{{url_for('usuario.MiPerfil', id=session['id'])}}" class="btn btn-success"><strong>Volver</strong></a>
</div>
<br><br>
{% if user['main'] != None %}
<div id="profileGreen" class="{{user['main']}}">
    <br><br>
    <div class="profileAvatar">
        <br>
        <img src="{{user['avatar']}}">
        <br><br>
    </div>
    <br>
    <div class="profileDatos">
        <br><br>
        <div>
            <label style="color: white;"><strong>Nombre:</strong> {{user["name"]}}</label>
        </div>
        <br><br>
        <div>
            <label style="color: white;"><strong>Mote:</strong> {{user["mote"]}}</label><br>
        </div>
        <br><br>
        <div>
            <label style="color: white;"><strong>Sobre mi:</strong></label>
            <br><br><br>
            <textarea id="textDescripcion" name="descripcion" readonly>
                        {{user["descripcion"]}}
                    </textarea>
            <br>
        </div><br>
    </div>
    <br><br>
</div>
{% else %}
<div id="profileGreen" class="profileDefault">
    <br><br>
    <div class="profileAvatar">
        <br>
        <img src="{{user['avatar']}}">
        <br><br>
    </div>
    <br>
    <div class="profileDatos">
        <br><br>
        <div>
            <label style="color: white;"><strong>Nombre:</strong> {{user["name"]}}</label>
        </div>
        <br><br>
        <div>
            <label style="color: white;"><strong>Mote:</strong> {{user["mote"]}}</label><br>
        </div>
        <br><br>
        <div>
            <label style="color: white;"><strong>Sobre mi:</strong></label>
            <br><br><br>
            <textarea id="textDescripcion" name="descripcion" readonly>
                        {{user["descripcion"]}}
                    </textarea>
            <br>
        </div><br>
    </div>
    <br><br>
</div>
{% endif %}
<br><br>
<br>
<div class="editPannel">
    <br><br>
    <div class="options">
        <div class="btn-group">
            <button id="buttonMenu" style="font-size: 19px; font-weight: bold;" type="button"
                class="btn btn-success dropdown-toggle" data-toggle="dropdown" aria-haspopup="true"
                aria-expanded="false">
                Estilos Normales
            </button>
            <div id="dOpts" class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                <button class="dropdown-item" id="everyDefaultBtn">Todos</button>
                <button class="dropdown-item" id="blueFilterBtn">Azul</button>
                <button class="dropdown-item" id="greenFilterBtn">Verde</button>
                <button class="dropdown-item" id="redFilterBtn">Rojo</button>
                <button class="dropdown-item" id="yellowFilterBtn">Amarillo</button>
                <button class="dropdown-item" id="purpleFilterBtn">Morado</button>
            </div>
            <script>
                let btnMenu = document.getElementById("buttonMenu");
                let menuOpts = document.getElementById("dOpts");
                btnMenu.addEventListener("click", () => {
                    menuOpts.classList.toggle("on");
                });
            </script>
        </div>
        <button id="opt2" class="btn btn-success"><strong>Estilos Especiales</strong></button>
    </div>
    <br><br><br><br>
    <div class="optionsSelect" id="optionsUserEdit">

        <!-- En este div aparecerÃ¡ todas las opciones disponibles de edicion de estilo -->
    </div>
    <br><br>
</div>
<br><br><br>
